menuconfig ETH_DWC_EQOS_QBU
	bool "IEEE 802.1Qbu (Frame Preemption)"
	depends on (ETH_DWC_EQOS_TX_QUEUES >= 2)
	default n
	help
	  Enable IEEE 802.1Qbu (Frame Preemption) support.

if ETH_DWC_EQOS_QBU

config ETH_DWC_EQOS_0_QBU
	bool "802.1Qbu support enable for port 0"
	depends on ETH_DWC_EQOS_0
	default n
	help
	  Enable IEEE 802.1Qbu (Frame Preemption) support for port 0.

config ETH_DWC_EQOS_0_QBU_HA
	int "Port 0: Hold advance (nanoseconds)"
	depends on ETH_DWC_EQOS_0_QBU
	default 0
	range 0 65535
	help
	  The maximum time in nanoseconds that can elapse between issuing
	  a HOLD to the MAC and the MAC ceasing to transmit any preemptable
	  frame.

config ETH_DWC_EQOS_0_QBU_RA
	int "Port 0: Release advance (nanoseconds)"
	depends on ETH_DWC_EQOS_0_QBU
	default 0
	range 0 65535
	help
	  The maximum time in nanoseconds that can elapse between issuing
	  a RELEASE to the MAC and the MAC being ready to resume transmission
	  of preemptable frames.

config ETH_DWC_EQOS_0_QBU_AFSZ
	int "Port 0: Additional fragment size"
	depends on ETH_DWC_EQOS_0_QBU
	default 0
	range 0 3
	help
	  Used to indicate, in units of 64 bytes, the minimum number of bytes
	  over 64 bytes required in non-final fragments of preempted frames.
	  Note: The minimum non-final fragment size is (AFSZ +1) * 64 bytes.

config ETH_DWC_EQOS_0_QBU_FPRQ
	int "Port 0: Frame preemption residue queue"
	depends on ETH_DWC_EQOS_0_QBU
	default 1
	range 1 7 if (ETH_DWC_EQOS_TX_QUEUES = 8)
	range 1 6 if (ETH_DWC_EQOS_TX_QUEUES = 7)
	range 1 5 if (ETH_DWC_EQOS_TX_QUEUES = 6)
	range 1 4 if (ETH_DWC_EQOS_TX_QUEUES = 5)
	range 1 3 if (ETH_DWC_EQOS_TX_QUEUES = 4)
	range 1 2 if (ETH_DWC_EQOS_TX_QUEUES = 3)
	range 1 1
	help
	  Rx queue number to which the residual preemption frames must be
	  forwarded/routed.
	  Note: Rx queue 0 is used as a default queue for express frames,
	  so this field cannot be programmed to a value 0.

config ETH_DWC_EQOS_0_QBU_ST
