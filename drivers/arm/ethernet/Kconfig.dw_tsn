menuconfig ETH_DWC_EQOS_QAV
	bool "IEEE 802.1Qav (Credit-Based Shaper)"
	depends on (ETH_DWC_EQOS_TX_QUEUES >= 2)
	default n
	help
	  Enable IEEE 802.1Qav (Credit-Based Shaper) support.

if ETH_DWC_EQOS_QAV

config ETH_DWC_EQOS_0_QAV
	bool "802.1Qav support enable for port 0"
	depends on ETH_DWC_EQOS_0
	default n
	help
	  Enable IEEE 802.1Qav (Credit-Based Shaper) support for port 0.
	  Note: Tx Queue 0 does not support CBS algorithm.

choice
	prompt "Port 0: Type of Qav Parameter"
	depends on ETH_DWC_EQOS_0_QAV
	default ETH_DWC_EQOS_0_QAV_TXQ_BANDWIDTH

config ETH_DWC_EQOS_0_QAV_TXQ_BANDWIDTH
	bool "bandwidth"
	help
	  Bandwidth reserved, in percentage.

config ETH_DWC_EQOS_0_QAV_TXQ_IDLESLOPE
	bool "idle slope"
	help
	  Rate of increase of credit, in Megabits per second.

endchoice

config ETH_DWC_EQOS_0_QAV_TXQ1_ENABLE
	bool "Port 0: Queue 1: CBS enable"
	depends on ETH_DWC_EQOS_0_QAV
	default n
	help
	  Enable IEEE 802.1Qav (Credit-Based Shaper) for Tx Queue 1.

config ETH_DWC_EQOS_0_QAV_TXQ1_BANDWIDTH
	int "Port 0: Queue 1: Bandwidth (%)"
	depends on ETH_DWC_EQOS_0_QAV_TXQ1_ENABLE && \
		   ETH_DWC_EQOS_0_QAV_TXQ_BANDWIDTH
	default 10
	range 1 100
	help
	  Bandwidth reserved for Tx Queue 1, in percentage.

config ETH_DWC_EQOS_0_QAV_TXQ1_IDLESLOPE
	int "Port 0: Queue 1: Idle slope (Mbps)"
	depends on ETH_DWC_EQOS_0_QAV_TXQ1_ENABLE && \
		   ETH_DWC_EQOS_0_QAV_TXQ_IDLESLOPE
	default 100
	range 1 1000
	help
	  Rate of increase of credit for Tx Queue 1, in Megabits per second.
	  Note: This value should not exceed link speed of port 0.

