menuconfig ETH_DWC_EQOS_TBS
	bool "TBS (Time-Based Scheduler)"
	depends on NET_PKT_TXTIME && NET_CONTEXT_TXTIME
	default n
	help
	  Enable TBS (Time-Based Scheduler) support.

if ETH_DWC_EQOS_TBS

config ETH_DWC_EQOS_TBS_FETCH_OFFSET
	int "Fetch Time Offset"
	default -1
	range -1 999999999
	help
	  Configure fetch time value in 256ns unit.
	  Value not in 256ns unit will be round down.
	  -1 to turn off fetch ahead support.

config ETH_DWC_EQOS_TBS_EXPIRY_OFFSET
	int "Launch Time Expiry Offset"
	default -1
	range -1 999999999
	help
	  Configure launch time expiry value in 256ns unit.
	  Value not in 256ns unit will be round down.
	  -1 to turn off launch expiry support.


config ETH_DWC_EQOS_0_TBS_TXQ0_ENABLE
	bool "Queue 0: TBS enable"
	depends on ETH_DWC_EQOS_0
	default n
	help
	  Enable TBS for Tx Queue 0.

