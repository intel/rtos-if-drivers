menuconfig ETH_DWC_EQOS_QBV
	bool "IEEE 802.1Qbv (Enhancements for Scheduled Traffic)"
	depends on (ETH_DWC_EQOS_TX_QUEUES >= 2)
	default n
	help
	  Enable IEEE 802.1Qbv (Enhancements for Scheduled Traffic) support.

if ETH_DWC_EQOS_QBV

config ETH_DWC_EQOS_0_QBV
	bool "802.1Qbv support enable for port 0"
	depends on ETH_DWC_EQOS_0
	default n
	help
	  Enable IEEE 802.1Qbv (Enhancements for Scheduled Traffic) support
	  for port 0.

config ETH_DWC_EQOS_0_QBV_GCL_DEPTH
	int "Port 0: Gate control list depth"
	depends on ETH_DWC_EQOS_0_QBV
	default 512
	range 2 1024
	help
	  Maximum length (number of row) of gate control list.

config ETH_DWC_EQOS_0_QBV_TILS
	int "Port 0: Time interval left shift"
	depends on ETH_DWC_EQOS_0_QBV
	default 0
	range 0 7
	help
	  The amount of left shift bits (equal to multiplication) of the
	  time interval in gate control list.

config ETH_DWC_EQOS_0_QBV_PTOV
	int "Port 0: PTP time offset (nanoseconds)"
	depends on ETH_DWC_EQOS_0_QBV
	default 0
	range 0 255
	help
	  The value of PTP clock offset needed to avoid transmission overruns
	  at the beginning of the installation of new in gate control list,
	  in nanosecond.

config ETH_DWC_EQOS_0_QBV_CTOV
	int "Port 0: Current time offset (nanoseconds)"
	depends on ETH_DWC_EQOS_0_QBV
	default 0
	range 0 4095
	help
	  The time added to the current time to compensate for all the
	  implementation pipeline delays, in nanosecond.

config ETH_DWC_EQOS_0_QBV_ENABLE
	bool "Port 0: EST enable"
	depends on ETH_DWC_EQOS_0_QBV
	default n
